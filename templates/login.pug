extends base.pug
block title 
    title login 

block content
    #loginContent
        form#loginForm(action="/login" method="POST")
            #form
                label(for="username") Username: 
                input#username(name="username")
            
                label(for="password") Password: 
                input#password(name="password")
                input#submit(name="submit" type="submit")